<h1><%= link_to @song.title, song_path(@song) %></h1>
<% if @song.artist %>
  <h3> by <%= display_artist(@song) %></h3>
  <p><%= link_to "Edit Song", edit_artist_song_path(@song.artist, @song) %></p>
<% else %>
  <p><%= link_to "Add Artist", edit_song_path(@song) %></p>
<% end %>
<p><%= link_to "All Songs", songs_path %></p>
